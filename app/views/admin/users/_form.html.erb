<div id="user_form">
  <%= form_for [:admin,@user] do |f| %>
    <%= f.label :email, 'Почта' %>
    <%= f.text_field :email %>
    
    <%= f.label :password, 'Пароль' %>
    <%= f.password_field :password %>
    
    <%= f.label :password_confirmation, 'Пароль еще раз' %>
    <%= f.password_field :password_confirmation %>
    
    <%= f.submit 'Сохранить' %>
  <% end %>
</div>
